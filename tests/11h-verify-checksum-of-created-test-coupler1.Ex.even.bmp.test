#!/bin/sh
# There should be a supplied bitmap file test-coupler1.bmp
# We will generate all the bitmaps and check they 
# are all okay. 
CKSUM=`$top_builddir/tools/src/mycksum $top_builddir/examples/test-coupler1.Ex.even.bmp`
MD5SUM=`md5sum $top_builddir/examples/test-coupler1.Ex.even.bmp | awk '{print $1}'`
##if [ $CKSUM != 09736 ]; then
#if [ $CKSUM != 38572 ]; then
#if [ $MD5SUM != 97ca91a111f474de4d5cde0a7c4138a2 ]; then
if [ $MD5SUM != 97ca91a111f474de4d5cde0a7c4138a2 ]; then
  echo "FAILED:" $0  >> tests.log
  echo "Failed checksum " $MD5UM $0
  echo "Failed checksum " $MD5UM $0 >> tests.log
  exit 1
else 
  echo "PASSED:" $0 >> tests.log
  exit 0
fi

