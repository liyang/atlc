../src/non_gui/generate_coupler_bitmap 1.0  1.0 1.0 1.0  ../examples/coupler1.bmp >/dev/null 2>/dev/null
../src/non_gui/generate_coupler_bitmap 2.0  1.0 1.0 1.0  ../examples/coupler2.bmp >/dev/null 2>/dev/null
../src/non_gui/generate_coupler_bitmap 3.0  1.0 1.0 1.0  ../examples/coupler3.bmp >/dev/null 2>/dev/null
../src/non_gui/generate_coupler_bitmap 4.0  1.0 1.0 1.0  ../examples/coupler4.bmp >/dev/null 2>/dev/null
../src/non_gui/generate_coupler_bitmap 5.0  1.0 1.0 1.0006  ../examples/coupler5.bmp >/dev/null 2>/dev/null
../src/non_gui/generate_coupler_bitmap 1.0  1.0 0.5 3.335  ../examples/coupler6.bmp >/dev/null 2>/dev/null
../src/non_gui/generate_coupler_bitmap 1.0  1.0 0.1 2.33  ../examples/coupler7.bmp >/dev/null 2>/dev/null
../src/non_gui/generate_coupler_bitmap 0.25 1.0 1.0 4.8  ../examples/coupler8.bmp >/dev/null 2>/dev/null
# Now check the length of the last file. It this is okay, it probably means
# the user has not run out of disk space. 

LENGTH=`../tools/src/myfilelength ../examples/coupler8.bmp`
if [ $LENGTH = 424762 ]; then
  echo "PASSED:" $0 >> tests.log
  exit 0
else
  echo "FAILED:" $0  >> tests.log
  exit 1
fi

