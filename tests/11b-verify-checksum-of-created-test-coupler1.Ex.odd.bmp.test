#!/bin/sh
# There should be a supplied bitmap file test-coupler1.bmp
# We will generate all the bitmaps and check they 
# are all okay. 

$top_builddir/src/non_gui/atlc $top_builddir/examples/test-coupler1.bmp  > /dev/null
CKSUM=`$top_builddir/tools/src/mycksum $top_builddir/examples/test-coupler1.Ex.odd.bmp`
if [ $CKSUM != 34975 ]; then
  echo "FAILED:" $0  >> tests.log
  exit 1
else 
  echo "PASSED:" $0 >> tests.log
  exit 0
fi

