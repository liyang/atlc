#!/bin/sh
# There should be a supplied bitmap file cop1.bmp
# We will generate all the bitmaps and check they 
# are all okay
CKSUM=`../tools/src/mycksum ../examples/cop1.Ey.even.bmp`
if [ $CKSUM != 02178 ]; then
  echo "FAILED:" $0  >> tests.log
  exit 1
else 
  echo "PASSED:" $0 >> tests.log
  exit 0
fi

