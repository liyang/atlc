#!/bin/sh
# There should be a supplied bitmap file test-coupler1.bmp
# We will generate all the binary files and check they 
# are all okay. 
CKSUM=`$top_builddir/tools/src/mycksum $top_builddir/examples/test-coupler1.Ex.even.bin`
if [ $CKSUM != 12326 ]; then
  echo "FAILED:" $0  >> tests.log
  echo "Failed checksum " $CKSUM $0
  exit 1
else 
  echo "PASSED:" $0 >> tests.log
  exit 0
fi

